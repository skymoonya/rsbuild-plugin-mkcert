{
  "name": "rsbuild-plugin-mkcert",
  "version": "0.0.0",
  "description": "Provide certificates for rsbuild's https dev service",
  "repository": {
    "type": "git",
    "url": "https://github.com/skymoonya/rsbuild-plugin-mkcert.git"
  },
  "keywords": [
    "rsbuild-plugin",
    "certificate",
    "https",
    "mkcert",
    "ssl"
  ],
  "author": "skymoonya",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/skymoonya/rsbuild-plugin-mkcert/issues"
  },
  "homepage": "https://github.com/skymoonya/rsbuild-plugin-mkcert#readme",
  "engines": {
    "node": ">=v16.7.0"
  },
  "packageManager": "pnpm@8.0.0+",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": ["dist"],
  "scripts": {
    "dev": "rsbuild dev -c playground/rsbuild.config.ts",
    "build": "unbuild",
    "lint": "tsc --noEmit && eslint \"src/**/*.ts\" --color --quiet",
    "release": "release-it"
  },
  "dependencies": {
    "@octokit/rest": "^20.0.2",
    "axios": "^1.6.5",
    "debug": "^4.3.4",
    "picocolors": "^1.0.0"
  },
  "devDependencies": {
    "@rsbuild/core": "^0.4.15",
    "@types/debug": "^4.1.12",
    "@types/node": "^20.10.7",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.2",
    "prettier": "^3.1.1",
    "release-it": "^17.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "unbuild": "^2.0.0"
  },
  "peerDependencies": {
    "@rsbuild/core": "*"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  }
}
